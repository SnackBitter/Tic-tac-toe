<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="application_1.controller.Controller">
   <Pane id="base" fx:id="base_square" layoutX="150.0" layoutY="50.0" prefHeight="300.0" prefWidth="300.0">
      <Rectangle fx:id="game_panel" arcHeight="5.0" arcWidth="5.0" fill="#f2f2f2" height="270.0" layoutX="15.0"
        layoutY="15.0" stroke="BLACK" strokeType="INSIDE" width="270.0"/>
      <Text fx:id="text" layoutX="18.0" layoutY="10.0"/>
      <Line endX="170.0" layoutX="115.0" layoutY="105.0" startX="-100.0"/>
      <Line endX="170.0" layoutX="115.0" layoutY="195.0" startX="-100.0"/>
      <Line endX="170.0" layoutX="70.0" layoutY="150.0" rotate="270.0" startX="-100.0"/>
      <Line endX="170.0" layoutX="160.0" layoutY="151.0" rotate="90.0" startX="-100.0"/>
      <Button fx:id="startButton" layoutX="-100.0" layoutY="60.0" text="Connect"/>
      <Button fx:id="closeButton" layoutX="-100.0" layoutY="100.0" text="DisConnect"/>
      <Button fx:id="Circle" layoutX="-100.0" layoutY="-20.0" text="Circle"/>
      <Button fx:id="Line" layoutX="-100.0" layoutY="20.0" text="Line"/>
   </Pane>
</AnchorPane>
